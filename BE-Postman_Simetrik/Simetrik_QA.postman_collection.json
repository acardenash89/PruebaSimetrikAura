		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const status = pm.environment.get(\"status\");",
					"const response = pm.response.json();",
					"",
					"//Como ejemplo se automatiza la respuesta tipo 200",
					"",
					"if(status == 200){",
					"    const count = pm.environment.get(\"count\");",
					"    const next = pm.environment.get(\"next\");",
					"    const previous = pm.environment.get(\"previous\");",
					"    const name = pm.environment.get(\"name\");",
					"    const url = pm.environment.get(\"url\");",
					"    ",
					"    pm.test(\"Status code is 200\", function () {",
					"        pm.response.to.have.status(200);",
					"    });",
					"",
					"    pm.test(\"Validate keys from response 200\", function () {",
					"        pm.expect(response).to.have.keys(\"count\", \"next\", \"previous\", \"results\");",
					"        pm.expect(response.results[0]).to.have.all.keys(\"name\", \"url\");",
					"    });",
					"",
					"    pm.test(\"Validate data from response 200\", function () {",
					"        pm.expect(response.count).to.eql(count);",
					"        pm.expect(response.next).to.eql(next);",
					"        pm.expect(response.previous).to.eql(previous);",
					"        pm.expect(response.results[0].name).to.eql(name);",
					"        pm.expect(response.results[0].url).to.eql(url);",
					"    });",
					"",
					"}"
				]
			}
		}
